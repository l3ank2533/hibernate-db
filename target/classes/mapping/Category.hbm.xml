<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.grandek.mydb.model.Category" table="CATEGORY">
        <meta attribute="class-description">This class contains category details.</meta>
        <id name="id" type="long" column="CATEGORY_ID">
            <generator class="native"/>
        </id>
        <property name="categoryName" type="string" length="50" not-null="true" column="CATEGORY_NAME"/>


    </class>

</hibernate-mapping>
